# Ignore Node.js modules and logs
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Environment variable files
.env

# Logs
logs
*.log

# Ignore build and coverage directories
dist/
coverage/

# Ignore specific generated files (if applicable)
*.js.map
*.css.map

# Ignore generated files in specific directories, but not source files
# (Adjust 'dist' to the correct build output directory if necessary)
keep-clone-app/dist/
backend/dist/

# Ignore IDE/editor config files
.vscode/
.idea/
*.sublime-project
*.sublime-workspace
.DS_Store

# Don't ignore source files
!keep-clone-app/src/**/*.js
!backend/**/*.js

# Ignore images in build directories, but not in source directories
dist/**/*.png
dist/**/*.svg
dist/**/*.jpg
dist/**/*.jpeg